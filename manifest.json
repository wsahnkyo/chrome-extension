{
  "manifest_version": 3,
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "hello.html",
    "default_icon": "images/hello_extensions.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/jquery-3.7.1.min.js","scripts/content.js"],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}